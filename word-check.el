(defun replace-word (pear)
  "replace word"
  (interactive)
  (goto-char (point-min))
  (query-replace-regexp (car pear) (car (cdr pear))))

(defun replace-words (pear)
  "replace many words"
  (interactive "*")
  (mapcar (function replace-word) pear)
  (goto-char (point-max)))

(replace-words words)
